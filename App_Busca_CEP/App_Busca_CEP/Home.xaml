<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App_Busca_CEP.Home"
             Style="{ StaticResource Fundo_Paginas }">
    
    <ContentPage.Content>

        <ScrollView VerticalOptions="Center">

            <StackLayout Padding="10,10,10,10" Spacing="15">

                <Image x:Name="img_logo" Aspect="AspectFit"/>

                <Button x:Name="btn_opcoes" Text="Ver Opções" Clicked="btn_opcoes_Clicked"/>

            </StackLayout>

        </ScrollView>
        
    </ContentPage.Content>
    
</ContentPage>